<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>

        <referenceBlock name='checkout.cart'>
            <action method='setTemplate'>
                <argument name='template' xsi:type='string'>Magento_Checkout::cart.phtml</argument>
            </action>
        </referenceBlock>

        <referenceBlock name='checkout.cart.methods.bottom'>
            <action method='setTemplate'>
                <argument name='template' xsi:type='string'>Magento_Checkout::cart/methods.phtml</argument>
            </action>
        </referenceBlock>

        <referenceBlock name='checkout.cart.empty'>
            <action method='setTemplate'>
                <argument name='template' xsi:type='string'>Magento_Checkout::cart/noItems.phtml</argument>
            </action>
        </referenceBlock>

        <referenceBlock name='checkout.cart.form'>
            <action method='setTemplate'>
                <argument name='template' xsi:type='string'>Vnecoms_VendorsSales::cart/form.phtml</argument>
            </action>
        </referenceBlock>
    </body>
</page>
